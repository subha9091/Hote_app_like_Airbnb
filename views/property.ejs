                                <% } %>

                                <% if(property.parking) { %>
                                <div><i class="fa-solid fa-car-rear" style="font-size: 24px; color:#222222"></i><span
                                        style="padding-left: 10px;">Free parking on premises</span></div>
                                <% } else {  %>
                                <div><i class="fa-solid fa-car-rear" style="font-size: 24px; color:#222222"></i><span
